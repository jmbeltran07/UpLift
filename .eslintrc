{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "new-cap": 0
  }
}
